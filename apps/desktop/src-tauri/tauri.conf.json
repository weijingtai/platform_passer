{
  "productName": "platform-passer",
  "version": "0.1.0",
  "identifier": "com.platform-passer.dev",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devUrl": null,
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "security": {
      "csp": null,
      "capabilities": [
        {
          "identifier": "main-capability",
          "description": "Main window capabilities",
          "windows": [
            "main"
          ],
          "permissions": [
            "core:event:allow-listen",
            "core:event:allow-emit",
            "core:webview:allow-internal-toggle-devtools",
            "notification:allow-is-permission-granted",
            "notification:allow-request-permission",
            "notification:allow-show"
          ]
        }
      ]
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Platform Passer",
        "width": 800,
        "height": 600
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}